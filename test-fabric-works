#Test it all works - NOTE that the database doesnt have to exist for fabric to start up
#can be created later
Create database shard7
Create(n:label) return n;

#BUT you do have to restart Fabric server IF you want to update the config

#query and use the id for the graph
USE fabric.graph(1)
MATCH (n) RETURN n

#the the name for the graph
USE fabric.july
MATCH (n) RETURN n

#you can iterate through graph ids
UNWIND fabric.graphIds() AS graphId
CALL {
  USE fabric.graph(graphId)
  MATCH (n)
  RETURN n  AS title limit 5
}
RETURN title