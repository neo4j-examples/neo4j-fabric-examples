#year is the aggregated view of the two graphs on remote servers
#graph 1 is jan
#graph 2 is feb

USE year.graph(1)
MATCH (n) RETURN n
  UNION
USE year.graph(2)
MATCH (n) RETURN n

UNWIND year.graphIds() AS graphId
CALL {
  USE year.graph(graphId)
  MATCH (n)
  RETURN n.name  AS title
}
RETURN title

#above makes use of two functions
<fabric database name>.graphIds()
<fabric database name>.graph(graphId)

USE year.graph(2)
MATCH (n) RETURN n
  UNION
USE year.graph(1)
MATCH (n) RETURN n

